{
  "Daemon": {
    "SerialPort": "/dev/ttyUSB0"
  },
  "Serilog": {
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "/var/log/D0Parser/D0Parser.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 104857600
        }
      }
    ]
  }
}